{
  "expo": {
    "name": "Cash",
    "slug": "cash",
    "scheme": "dotcash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dot.cash",
      "buildNumber": "1.0.0",
      "requireFullScreen": true,
      "config": {
        "branch": {
          "apiKey": "key_test_ga3Aj1x4tkuBHIqF1488tndprxbmSE5z"
        }
      },
      "infoPlist": {
        "NSFaceIDUsageDescription": "Dot requires FaceID to authenticate access to users' ewallets"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "dotcash.app.link",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.dot.cash",
      "versionCode": 1,
      "config": {
        "branch": {
          "apiKey": "key_test_ga3Aj1x4tkuBHIqF1488tndprxbmSE5z"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
